
{
  "id": "bc-template-quantum",
  "description": "Centralized authentication and authorization service for OpenStack",
  "attributes": {
    "quantum": {
      "debug": true,
      "verbose": true,
      "use_syslog": false,
      "sql_engine": "mysql",
      "mysql_instance": "none",
      "db": {
        "database": "quantum",
        "user": "quantum"
      },
      "sql": {
        "idle_timeout": 30,
        "min_pool_size": 5,
        "max_pool_size": 10,
        "pool_timeout": 200
      },
      "api": {
        "service_port": 5000,
        "service_host": "0.0.0.0"
      },
      "admin": {
        "tenant": "admin",
        "username": "admin",
        "password": "crowbar"
      },
      "service": {
        "tenant": "service",
        "token": "123456789123"
      },
      "default": {
        "tenant": "openstack",
        "username": "crowbar",
        "password": "crowbar"
      }
    }
  },
  "deployment": {
    "quantum": {
      "crowbar-revision": 0,
      "element_states": {
        "quantum-server": [ "readying", "ready", "applying" ]
      },
      "elements": {},
      "element_order": [
         ["quantum-server" ]       		           		     
      ],
      "config": {
        "environment": "quantum-config-base",
        "mode": "full",
        "transitions": false,
        "transition_list": []
      }
    }
  }
}

